{
    "name": "slashequip/paddle-php-sdk",
    "description": "PHP SDK for the Paddle.com API.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "SlashEquip\\PaddlePhpSdk\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Sam Jones",
            "email": "sam@slashequip.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "sammyjo20/saloon": "^1.4"
    },
    "require-dev": {
        "pestphp/pest": "^1.22",
        "phpstan/phpstan": "^1.8"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
